<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="entity">
	<class name="Reader">
		<id name="id" type="long">
			<generator class="native" />
		</id>
		<property name="createTime" not-null="true"/>
		<property name="username" type="string" not-null="true" length="50"/>
		<property name="email" type="string" not-null="true" length="50"/>
		<property name="password" type="string" not-null="true" length="50"/>
		<property name="repeatpw" type="string" not-null="true" length="50"/>
	</class>
</hibernate-mapping>